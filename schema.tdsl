database User

table User:
    id: int primary
    firstName: string
    lastName: string
    age: int
        constraint age <= 18 || id >= age
    phone: string?
        constraint "\\d{8}"
    email: string
        constraint "^\\S+@\\S+\\.\\S+$"
    address: Address?


table Address:
    id: string primary
    street: string
        constraint "^\\S+@\\S+\\.\\S+$"
    zipcode: int
    city: string
